<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ReportsScreenController">
    <children>
        <HBox prefHeight="29.0" prefWidth="121.0">
            <children>
                <Button mnemonicParsing="false" onMouseClicked="#navigateToMainScreen" style="-fx-background-color: white; -fx-border-color: black;" text="Home">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#handleExit" prefHeight="27.0" prefWidth="57.0" style="-fx-background-color: white; -fx-border-color: black;" text="Exit" />
            </children>
        </HBox>
        <AnchorPane layoutX="23.0" layoutY="33.0" prefHeight="335.0" prefWidth="555.0">
            <children>
                <HBox alignment="CENTER" layoutX="20.0" layoutY="90.0" prefHeight="140.0" prefWidth="506.0" spacing="20.0">
                    <children>
                        <Button mnemonicParsing="false" onMouseClicked="#appointmentsReportButtonClicked" prefHeight="140.0" prefWidth="149.0" text="Number of &#10;Appointments &#10;By Type &#10;and Month" textAlignment="CENTER">
                            <font>
                                <Font name="Arial" size="16.0" />
                            </font>
                        </Button>
                        <Button mnemonicParsing="false" onMouseClicked="#scheduleForContactsReportClicked" prefHeight="140.0" prefWidth="149.0" text="Schedule for&#10;Contacts" textAlignment="CENTER">
                            <font>
                                <Font name="Arial" size="16.0" />
                            </font>
                        </Button>
                        <Button mnemonicParsing="false" onMouseClicked="#loginInformationButtonClicked" prefHeight="140.0" prefWidth="149.0" text="Login &#10;Information" textAlignment="CENTER">
                            <font>
                                <Font name="Arial" size="16.0" />
                            </font>
                        </Button>
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                </HBox>
                <Label layoutX="228.0" layoutY="36.0" text="Reports">
                    <font>
                        <Font name="Arial Bold" size="24.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
